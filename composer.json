{
    "name": "fm-labs/cakephp-firewall",
    "description": "Firewall plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4",
        "shieldon/shieldon": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Firewall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firewall\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
